{
  "name": "sveltekit-electron",
  "version": "0.0.2",
  "private": true,
  "description": "Minimal Sveltekit + Electron starter template.",
  "main": "dist/main/main.cjs",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev npm run dev:all",
    "dev:all": "concurrently -n=svelte,electron -c='#ff3e00',blue \"npm run dev:svelte\" \"npm run dev:electron\"",
    "dev:svelte": "vite dev",
    "dev:electron": "npm run vite && electron dist/main/main.cjs",
    "build": "cross-env NODE_ENV=production npm run build:svelte && npm run build:electron",
    "build:svelte": "vite build",
    "build:electron": "npm run vite && electron-builder -l --config build.config.json",
    "vite:preload": "vite build -c vite.preload.config.js",
    "vite:electron": "vite build -c vite.electron.config.js",
    "vite": "npm run vite:preload && npm run vite:electron",
    "prepare": "svelte-kit sync",
    "lint": "eslint --ext .ts . && npm run prettier",
    "prettier": "prettier --check .",
    "fix": "eslint --ext .ts --fix . && prettier --write .",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "test:watch": "npm run test -- --watch",
    "typeorm": "typeorm-ts-node-commonjs",
    "postinstall": "electron-builder install-app-deps"
  },
  "engines": {
    "npm": ">=7",
    "yarn": "use npm - https://github.com/FractalHQ/sveltekit-electron/issues/12#issuecomment-1068399385"
  },
  "browserslist": [
    "Chrome 89"
  ],
  "dependencies": {
    "@trpc/client": "^9.27.2",
    "@trpc/server": "^9.27.2",
    "cors": "^2.8.5",
    "depcheck": "^1.4.3",
    "electron-context-menu": "^3.3.0",
    "electron-serve": "^1.1.0",
    "electron-window-state": "^5.0.3",
    "express": "^4.18.1",
    "jsdom": "^20.0.2",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.25.2",
    "sqlite3": "^5.1.2",
    "trpc-sveltekit": "^2.3.4",
    "zod": "^3.19.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.0",
    "@sveltejs/adapter-static": "1.0.0-next.36",
    "@sveltejs/kit": "1.0.0-next.372",
    "@testing-library/svelte": "^3.0.0",
    "@types/node": "^16.11.10",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "@vitest/coverage-c8": "^0.24.3",
    "carbon-components-svelte": "^0.70.6",
    "concurrently": "^7.2.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "electron": "^19.0.8",
    "electron-builder": "^23.1.0",
    "electron-connect": "^0.6.3",
    "electron-packager": "^15.5.1",
    "electron-rebuild": "^3.2.9",
    "electron-updater": "^5.0.5",
    "eslint": "^8.19.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-svelte": "^2.2.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "sass": "^1.53.0",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.0",
    "svelte-preprocess": "^4.10.7",
    "typescript": "4.5.2",
    "vite": "^2.9.14",
    "vitest": "^0.24.3"
  }
}
