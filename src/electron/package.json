{
   "name": "sveltekit-electron",
   "version": "0.0.1",
   "private": true,
   "description": "Minimal Sveltekit + Electron starter template.",
   "main": "dist/main.js",
   "type": "commonjs",
   "scripts": {
      "dev": "cross-env NODE_ENV=dev npm run dev:all",
      "dev:all": "concurrently -n=svelte,electron -c='#ff3e00',blue \"npm run dev:svelte\" \"npm run dev:electron\"",
      "dev:svelte": "vite dev",
      "dev:electron": "npm run dev:transpile && electron dist/electron.js",
      "dev:transpile": "tsc -p tsconfig.electron.json #&& mv dist/electron.js dist/electron.cjs && mv dist/data-source.js dist/data-source.cjs",
      "build": "cross-env NODE_ENV=production npm run build:svelte && npm run build:electron",
      "build:svelte": "vite build",
      "build:electron": "electron-builder -l --config build.config.json",
      "prepare": "svelte-kit sync",
      "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest src --config jest.config.json",
      "test:watch": "npm run test -- --watch",
      "typeorm": "typeorm-ts-node-commonjs"
   },
   "engines": {
      "npm": ">=7",
      "yarn": "use npm - https://github.com/FractalHQ/sveltekit-electron/issues/12#issuecomment-1068399385"
   },
   "browserslist": [
      "Chrome 89"
   ],
   "dependencies": {
      "better-sqlite3": "^7.6.2",
      "electron-context-menu": "^3.3.0",
      "electron-reloader": "^1.2.3",
      "electron-serve": "^1.1.0",
      "electron-window-state": "^5.0.3",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.3.9",
      "pg": "^8.4.0"
   },
   "devDependencies": {
      "@babel/core": "^7.14.0",
      "@babel/preset-env": "^7.14.0",
      "@sveltejs/adapter-static": "1.0.0-next.36",
      "@sveltejs/kit": "1.0.0-next.372",
      "@testing-library/jest-dom": "^5.14.0",
      "@testing-library/svelte": "^3.0.0",
      "@types/jest": "^27.5.2",
      "@types/testing-library__jest-dom": "^5.14.0",
      "@typescript-eslint/eslint-plugin": "^5.30.6",
      "@typescript-eslint/parser": "^5.30.6",
      "babel-jest": "^27.0.0",
      "concurrently": "^7.2.2",
      "cross-env": "^7.0.3",
      "dotenv": "^16.0.1",
      "electron": "^19.0.8",
      "electron-builder": "^23.1.0",
      "electron-connect": "^0.6.3",
      "electron-packager": "^15.5.1",
      "electron-updater": "^5.0.5",
      "eslint": "^8.19.0",
      "eslint-plugin-svelte": "^2.2.0",
      "jest": "^27.0.0",
      "npm-run-all": "^4.1.5",
      "prettier": "^2.7.1",
      "prettier-plugin-svelte": "^2.7.0",
      "sass": "^1.53.0",
      "svelte": "^3.49.0",
      "svelte-check": "^2.8.0",
      "svelte-jester": "^2.0.1",
      "svelte-preprocess": "^4.10.7",
      "ts-jest": "^27.0.0",
      "typescript": "4.5.2",
      "vite": "^2.9.14",
      "ts-node": "10.7.0",
      "@types/node": "^16.11.10"
   }
}
